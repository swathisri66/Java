public class ForLoopContinueLabel
{
public static void main(String argv[])
{
	for(int i=1;i<=3;i++)
	{
		for(int j=1;j<=3;j++)
		{
			if((i==2)&&(j==2))
			{
				System.out.println();
				continue label;
			}
			System.out.println(""+i+","+j+") ");
		}
		System.out.println();
	}
	System.out.println("End of the loop");
}
}
