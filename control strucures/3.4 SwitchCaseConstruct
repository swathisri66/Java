public class SwitchCaseConstruct
{
public static void main (String argv[])
  {
  int i;
  char ch=(char)-1;
  System.out.println("Enter an alphabet:");
  try
  {
	  ch=(char)System.in.read();
  }catch(Exception e)
  {
  }
  switch(ch)
  {
	case 'x' . i=10;
		break;
	case 'y' : i=20;
		break;
	case 'z' : i=30;
		break;
	default : i=40;
		break;
  }
  System.out.println("number :"+i);
  }
}
