/* Program to print the Factorial of Given Number */

class Factorial
{
 public static void main(String args[])
 {
  int n = 5,fact = 1;
  for(int i=1;i<=n;i++)
    fact = fact * i;
  System.out.printf("Factorial of %d is : %d",n,fact);
//  System.out.println("Factorial of " + n + " is : " + fact);
 } // main()
} // class
