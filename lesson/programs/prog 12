/* Program to print the multiplication table of a given number */

class TableDemo
{
 public static void main(String args[])
 {
  int n = 10;
  System.out.println("Multiplication Table of " + n + " is : ");
  for(int i=1;i<=10;i++)
    System.out.printf(" %d X %d = %d \n",n,i,n*i);
 } // main()
} // class
