/* Java program to draw solid shapes */


import java.applet.*;
import java.awt.*;
public class Example4 extends java.applet.Applet 
{
	public void paint(Graphics g)
	{
		g.setColor(Color.blue);
		g.fillRect(50,50,100,150);
		g.setColor(Color.red);
		g.fillOval(200,50,150,100);
	}
	public void start()
	{
		repaint();
	}
}
