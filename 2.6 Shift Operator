public class ShiftOperators
	{
	public static void main(String argv[])
		{
			int x=12,y=13;
			System.out.println(" x="+x);
			System.out.println(" y="+y);
			System.out.println("x<<2="+(x<<2));
			System.out.println("y<<2="+(y<<2));
			System.out.println("x>>2="+(x>>2));
			System.out.println("y>>2="+(y>>2));
			System.out.println("x>>>2="+(x>>>2));
			System.out.println("y>>>2="+(y>>>2));
		}
	}
