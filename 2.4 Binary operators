public class BasicBinaryOperators
{
	public static void main(String argv[])
	{
		int x=9, y=5;
		System.out.println(" x="+x);
		System.out.println(" y="+y);
		System.out.println("x+y="+ (x+y));
		System.out.println("x-y="+ (x-y));
		System.out.println("x/y="+ (x/y));
		System.out.println("x*y="+ (x*y));
		System.out.println("x%y="+ (x%y));
	}
}
	
